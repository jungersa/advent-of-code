package main

import (
	"bufio"
	"fmt"
	"os"
	"strconv"
	"strings"
)

type Range struct {
	start int
	end   int
}

func main() {
	file, err := os.Open("input.txt")
	if err != nil {
		fmt.Printf("Error opening input file: %v\n", err)
		return
	}
	defer file.Close()

	scanner := bufio.NewScanner(file)

	ranges := []Range{}
	for scanner.Scan() {
		line := scanner.Text()
		if line == "" {
			break
		}
		getPreData(line, &ranges)
	}

	result := 0
	for scanner.Scan() {
		line := scanner.Text()
		if line == "" {
			break
		}
		index, _ := strconv.Atoi(line)
		for _, r := range ranges {
			if index >= r.start && index <= r.end {
				result++
				break
			}
		}
	}

	if err := scanner.Err(); err != nil {
		fmt.Printf("Scanner error: %v\n", err)
		return
	}
	fmt.Println(result)
}

func getPreData(line string, ranges *[]Range) {
	a := strings.Split(line, "-")
	start, _ := strconv.Atoi(a[0])
	end, _ := strconv.Atoi(a[1])
	*ranges = append(*ranges, Range{start, end})
}
